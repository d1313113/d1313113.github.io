  },
  "dependencies": {
    "vuepress-plugin-cat": "^1.0.3",
    "vuepress-theme-meteorlxy": "^1.4.0"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged",
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS"
    }
  },
  "lint-staged": {
    "*.{js,vue}": [
      "eslint --fix",
      "git add"
    ]
  },
  "commitlint": {
    "extends": [
      "@commitlint/config-conventional"
    ]
  },
  "eslintConfig": {
    "extends": [
      "standard",
      "plugin:vue/recommended"
    ],
    "root": true,
    "rules": {
      "dot-notation": [
        "error",
        {
          "allowPattern": "^$"
        }
      ],
      "comma-dangle": [
        "error",
        "always-multiline"
      ]
    }
  }
}
